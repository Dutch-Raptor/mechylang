
let fib = fn(n) {
  if (n == 0) {
    return 0;
  }

  if (n == 1) {
    return 1;
  }

  return fib(n - 1) + fib(n - 2);
};

print("Recursive Fibonacci: fib(10) = ", fib(10));

// Iterative Fibonacci
let fib = fn(n) {
  let a = 0;
  let b = 1;

  for i in 0..n {
    b = a + b;
    a = b - a;
  }

  return a;
};

print("Iterative Fibonacci: fib(10) = ", fib(10));
